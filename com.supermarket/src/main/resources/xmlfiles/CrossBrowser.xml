<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="crossbrowser" thread-count="5" parallel="tests">
	<listeners>
		<listener class-name="com.supermarket.listeners.ExtentReporter"></listener>
	</listeners>
	<test name="reggressionchrome">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="com.supermarket.testes.LoginTest"></class>
			<class name="com.supermarket.testes.ManageCodTest"></class>
		</classes>
	</test>
	<test name="reggressionedge">
		<parameter name="browser" value="edge"></parameter>
		<classes>
			<class name="com.supermarket.testes.ManageProductTest"></class>
			<class name="com.supermarket.testes.PushNotificationsTest"></class>
		</classes>
	</test>
	<test name="reggressionfirefox">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="com.supermarket.testes.ManageProductTest"></class>
			<class name="com.supermarket.testes.PushNotificationsTest"></class>
		</classes>
	</test>
</suite>