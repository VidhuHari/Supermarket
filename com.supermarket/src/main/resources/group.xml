<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="grouprun" thread-count="5" parallel="tests">
	<listeners>
		<listener class-name="com.supermarket.listeners.ExtentReporter"></listener>
	</listeners>
	<test name="sanityrun">
		<groups>
		<run>
		<include name="smoke"></include>
		</run>
		</groups>
		<classes >
			<class name="com.supermarket.testes.LoginTest"></class>
			<class name="com.supermarket.testes.AdminUsersTest"></class>
			<class name="com.supermarket.testes.ManageCodTest"></class>
			<class name="com.supermarket.testes.PushNotificationsTest"></class>
		</classes >
	</test>
</suite>